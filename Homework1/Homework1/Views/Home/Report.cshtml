@model IEnumerable<Homework1.Models.SampleReport>

@{
    ViewBag.Title = "Report";
}

<h2>Report</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.客戶名稱)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.contactCount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.bankCount)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.客戶名稱)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.contactCount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.bankCount)
        </td>
    </tr>
}

</table>
